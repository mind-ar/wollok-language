/* XPECT_SETUP org.uqbar.project.wollok.tests.typesystem.xpect.TypeSystemXpectTestCase END_SETUP */

object consoleTest {

	// XPECT methodType at m1 --> (Any) => Void 
	method m1(a) {
	 	console.println(a)
	}
	
	// XPECT methodType at readLine --> () => String 
	method readLine() = console.readLine()
	
	// XPECT methodType at readInt --> () => Number 
	method readInt() = console.readInt()
	
	// XPECT methodType at newline --> () => String 
	method newline() = console.newline()
}

program p {
	// XPECT type at console --> console 
 	console

	// XPECT type at "hola" --> String
 	console.println("hola") 	
}

