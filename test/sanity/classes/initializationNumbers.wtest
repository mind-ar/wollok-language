/**
  * Scenario: A Class with attributes pointing to numbers
  */
class A {
  const i = 10
  const j = 0.10

  method getI() = i
  method getJ() = j
}

/**
  * Tests
  */
test "clases - attributes number initialization" {
  const a = new A()
  assert.equals(10, a.getI())
  assert.equals(0.10, a.getJ())
}
